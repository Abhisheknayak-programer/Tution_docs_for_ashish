  <!-- Contact Page Design -->
  <section class="ContactArea">
    <div class="container">
      <h1 style="text-align: center;">Contact Us</h1>
        <div class="row">
            <div class="col col-sm-12 col-lg-6 col-md-8">
                <form action="/contact" method="POST">
                    
                     <div class="mb-3">
                      <label for="exampleInputEmail1" class="form-label">Name</label>
                      <input type="text" class="form-control" autocomplete="none" name="name" required>
                    </div>

                    <div class="mb-3">
                      <label for="exampleInputEmail1" class="form-label">Email address</label>
                      <input type="email" class="form-control" autocomplete="none" name="email" required>
                      <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
                    </div>

                    <div class="mb-3">
                        <label for="exampleInputPassword1" class="form-label">Mobile Number</label>
                        <input type="number" class="form-control" autocomplete="none" name="number" required>
                    </div>

                    <div class="mb-3">
                      <label for="exampleInputPassword1" class="form-label">Subject</label>
                      <input type="text" class="form-control" name="subject" required>
                    </div>

                    <div class="mb-3">
                      <label for="exampleInputPassword1" class="form-label">Enter The Description</label>
                      <input type="text" class="form-control" name="desc" required>
                      <div id="emailHelp" class="form-text">Genuine Queries Only</div>
                    </div>


                    <div class="mb-3 form-check">
                      <input type="checkbox" class="form-check-input" name="simpleuser" id="exampleCheck1">
                      <label class="form-check-label" for="exampleCheck1">I am a simple user</label>
                    </div>
                    
                    
                    <div class="mb-3 form-check">
                        <input type="checkbox" class="form-check-input" name="artist" id="exampleCheck1">
                        <label class="form-check-label" for="exampleCheck1">I am an artist</label>
                    </div>
                      
                    <button type="submit" class="btn btn-primary">Submit</button>
                  </form>
            </div>
        </div>
    </div>  
  </section>
 



